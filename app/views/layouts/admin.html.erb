    <link href="https://res.cloudinary.com/chota5511/raw/upload/v1537433484/MyCV/stylesheets/admin/animate.css" rel="stylesheet" media="all">
    <link href="https://res.cloudinary.com/chota5511/raw/upload/v1537433484/MyCV/stylesheets/admin/hamburgers.min.css" rel="stylesheet" media="all">
