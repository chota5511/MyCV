    <script src="https://res.cloudinary.com/chota5511/raw/upload/v1537424154/MyCV/javascripts/admin/wow.min.js"></script>
    <script src="https://res.cloudinary.com/chota5511/raw/upload/v1537424154/MyCV/javascripts/admin/animsition.min.js"></script>
    <script src="https://res.cloudinary.com/chota5511/raw/upload/v1537424154/MyCV/javascripts/admin/bootstrap-progressbar.min.js"></script>
    <script src="https://res.cloudinary.com/chota5511/raw/upload/v1537424154/MyCV/javascripts/admin/jquery.waypoints.min.js"></script>
    <script src="https://res.cloudinary.com/chota5511/raw/upload/v1537424154/MyCV/javascripts/admin/jquery.counterup.min.js"></script>
    <script src="https://res.cloudinary.com/chota5511/raw/upload/v1537424154/MyCV/javascripts/admin/circle-progress.min.js"></script>
    <script src="https://res.cloudinary.com/chota5511/raw/upload/v1537424154/MyCV/javascripts/admin/perfect-scrollbar.js"></script>
    <script src="https://res.cloudinary.com/chota5511/raw/upload/v1537424154/MyCV/javascripts/admin/Chart.bundle.min.js"></script>
    <script src="https://res.cloudinary.com/chota5511/raw/upload/v1537424154/MyCV/javascripts/admin/select2.min.js"></script>
    <script src="https://res.cloudinary.com/chota5511/raw/upload/v1537424154/MyCV/javascripts/admin/jquery.vmap.js"></script>
    <script src="https://res.cloudinary.com/chota5511/raw/upload/v1537424154/MyCV/javascripts/admin/jquery.vmap.min.js"></script>
    <script src="https://res.cloudinary.com/chota5511/raw/upload/v1537424154/MyCV/javascripts/admin/jquery.vmap.sampledata.js"></script>
    <script src="https://res.cloudinary.com/chota5511/raw/upload/v1537424154/MyCV/javascripts/admin/jquery.vmap.world.js"></script>
    <!-- Main JS-->
    <script src="https://res.cloudinary.com/chota5511/raw/upload/v1537424154/MyCV/javascripts/admin/main.js"></script>

</body>

